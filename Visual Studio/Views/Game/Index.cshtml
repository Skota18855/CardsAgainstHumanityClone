@model CardsAgainstHumanityClone.Models.Profile
@{
    ViewData["Title"] = "GamePage";
}
<h1>Welcome, @Model.UserName</h1>


<link rel="stylesheet" type="text/css" href="~/build/TemplateData/style.css" />
<script src="~/build/TemplateData/UnityProgress.js"></script>
<script src="~/build/Build/UnityLoader.js"></script>


<script>
    var gameInstance = UnityLoader.instantiate("gameContainer", "~/build/Build/build.json", { onProgress: UnityProgress });
</script>

<div class="webgl-content">
    <div id="gameContainer" style="width: 960px; height: 600px"></div>
    <div class="footer">
        <div class="webgl-logo"></div>
        <div class="fullscreen" onclick="gameInstance.SetFullscreen(1)"></div>
        <div class="title">Figurines</div>
    </div>
</div>